#ifndef CARRELLO_H_
#define CARRELLO_H_



typedef enum items_type
{

	FRUTTA,
	VERDURA,
	PANE,
	LATTE

}items_type;


class carrello {
public:
	carrello();
	virtual ~carrello();


	list<items_type> items_local;
	void aggiunta_item(items_type it);
};

#endif /* CARRELLO_H_ */
